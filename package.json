{
  "name": "Backborn-WebPack-Babel",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "npm-run-all -p start:*",
    "start:watch": "npm-run-all -p watch:*",
    "start:nodeJS": "node dev.js",
    "build": "npm-run-all clean mkdir sprite --print-label -p build:img build:js --print-label",
    "build:img": "npm-run-all img:copy img:min",
    "build:js": "npm-run-all js:webpack -p",
    "clean": "npm-run-all -p clean:*",
    "clean:img": "rimraf htdocs/img htdocs/src/img/sprite-*.png",
    "clean:css": "rimraf htdocs/css htdocs/src/styl/sprite/*",
    "clean:js": "rimraf htdocs/js",
    "mkdir": "npm-run-all -p mkdir:*",
    "mkdir:img": "mkdirp htdocs/img",
    "mkdir:css": "mkdirp htdocs/css",
    "mkdir:js": "mkdirp htdocs/js",
    "js:webpack": "webpack --config .webpack.config.js",
    "sprite": "spritesmith --rc .spritesmith.js",
    "img:copy": "cpx \"htdocs/src/img/**/*.{png,gif,jpg}\" htdocs/img",
    "img:min": "imagemin -P pngquant htdocs/img htdocs/img/",
    "watch:js": "webpack --config .webpack.watch.config.js"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "jquery": "^3.1.0",
    "underscore": "^1.8.3"
  },
  "author": "skyphic",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-module-alias": "^1.4.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "chokidar-cli": "^1.2.0",
    "clean-css": "^3.4.19",
    "cpx": "^1.3.1",
    "express": "^4.14.0",
    "imagemin-cli": "^3.0.0",
    "imagemin-pngquant": "^5.0.0",
    "mkdirp": "^0.5.1",
    "nib": "^1.1.0",
    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.4",
    "slick-carousel": "^1.6.0",
    "spritesmith-cli": "^1.1.2",
    "stylus": "^0.54.2",
    "uglify-js": "^2.6.2",
    "uglify-save-license": "^0.4.1",
    "webpack": "^1.12.15"
  }
}
